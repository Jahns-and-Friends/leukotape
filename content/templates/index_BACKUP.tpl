        <script>
            $(document).ready(function() {
                var $videoSrc;
                var dropContainer = document.getElementById("dropContainer");
                var fileInput = document.getElementById("fileInput");

                $('.videobox').click(function() {
                    $videoSrc = $(this).find("img").data( "src" );
                    console.log($videoSrc);
                });
                // when the modal is opened autoplay it
                $('#myModal').on('shown.bs.modal', function (e) {
                // set the video src to autoplay and not to show related video. Youtube related video is like a box of chocolates... you never know what you're gonna get
                $("#video").attr('src',$videoSrc + "?rel=0&amp;showinfo=0&amp;modestbranding=1&amp;autoplay=1" );
                })
                // stop playing the youtube video when I close the modal
                $('#myModal').on('hide.bs.modal', function (e) {
                // a poor man's stop video
                $("#video").attr('src',$videoSrc);
                });

                // dragover and dragenter events need to have 'preventDefault' called
                // in order for the 'drop' event to register.
                // See: https://developer.mozilla.org/en-US/docs/Web/Guide/HTML/Drag_operations#droptargets
                dropContainer.ondragover = dropContainer.ondragenter = function(evt) {
                    evt.preventDefault();
                    dropContainer.style.border = "6px dashed #F50082";
                };
                dropContainer.ondragleave = function(evt) {
                    evt.preventDefault();
                    dropContainer.style.border = "0px dashed #F50082";
                };
                dropContainer.ondrop = function(evt) {
                    // pretty simple -- but not for IE :(
                    dropContainer.style.border = "0px dashed #F50082";
                    console.log(evt.dataTransfer.files);
                    fileInput.files = evt.dataTransfer.files;
                    loadFile(fileInput.files);
                    evt.preventDefault();
                };
            });
            var loadFile = function(files) {
                /*console.log(files);
                var output = document.getElementById('output');
                output.src = URL.createObjectURL(files[0]);
                output.classList.remove("invisible");
                output.classList.add("visible");*/
                for (let i = 0; i < files.length; i++) {
                    const file = files[i];

                    if (!file.type.startsWith('image/')){ continue }
                    //console.log(file);

                    const img = document.createElement("img");
                    const output = document.getElementById('output');

                    img.classList.add("obj");
                    img.file = file;
                    output.innerHTML = '';
                    output.appendChild(img);
                    output.src = URL.createObjectURL(file);

                    const reader = new FileReader();
                    reader.onload = (function(aImg) { return function(e) { aImg.src = e.target.result; }; })(img);
                    reader.readAsDataURL(file);
                }
            };
        </script>
